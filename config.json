{
  "PhoneAsItem": {
    "enabled": true,
    "exportResource": "blrp_core",
    "exportFunction": "npwdHasPhone"
  },
  "customPhoneNumber": {
    "enabled": false,
    "exportResource": "number-generator-resource",
    "exportFunction": "generateNumber"
